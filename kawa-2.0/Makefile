dist-kawa-2.0.jar: $(DIST_ARCHIVES)
	srcdir=`(cd $(srcdir); /bin/pwd)` \
	  && rm -rf tmpdir \
	  && mkdir tmpdir \
	  && cd tmpdir \
	  && tar xzf ../$(distdir).tar.gz \
	  && rm -f ./patch-source-list \
	  && ./kawa-2.0/configure --with-java-source=7 \
	    --with-servlet= --with-javafx= \
	  && CLASSPATH=.$(CLASSPATH_SEPARATOR)$$CLASSPATH $(MAKE) \
	  && mv kawa-2.0.jar ../dist-kawa-2.0.jar \
	  && cd .. && rm -rf tmpdir

